import java.util.Scanner;
public class FazerCalculo {
    public static void main(String[] args){

        Scanner scanner= new Scanner(System.in);
        
        System.out.print("Valor 1: ");
        Double Nota1 = scanner.nextDouble();
        System.out.print("Valor 2: ");
        Double Nota2 = scanner.nextDouble();
        scanner.close();
    System.out.println("Valor Somado: " + (Nota1 + Nota2));
System.out.println("Valor Subtraido: " + (Nota1 - Nota2));
System.out.println("Valor Dividido: " + (Nota1 / Nota2));
System.out.println("Valor Multiplicado: " + (Nota1 * Nota2));
    }
}
